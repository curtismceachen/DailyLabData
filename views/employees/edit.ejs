<%- include('../partials/header') %>
<h2>Edit Lab Entry</h2>

<form action="/labs/<%= lab._id %>?_method=PUT" method="POST">

  <label>Reactive phosphorous
    <input type ="number" step="0.01" name="reactPh" value="<%= lab.reactPh %>">
  </label><br>
  <label>pH
    <input type ="number" step="0.01" name="pH" value="<%= lab.pH %>">
  </label><br>
  <label>Ammonia
    <input type ="number" step="0.01" name="ammonia" value="<%= lab.ammonia %>">
  </label><br>
  <label>Temperature
    <input type ="number" step="0.01" name="temperature" value="<%= lab.temperature %>">
  </label><br>
  <label>Dissolved oxygen
    <input type ="number" step="0.01" name="dissOx" value="<%= lab.dissOx %>">
  </label><br>
  <label>MLSS
    <input type="number" step="0.01" name="mlss" value="<%= lab.mlss %>">
  </label><br>
  <label>Waste Volume (m3)
    <input type="number" step="0.01" name="wasteVol" value="<%= lab.wasteVol %>">
  </label><br>
  </label>
  <label>Date
    <input type="date" name="date" value="<%= lab.date %>">
  </label><br>
  <label>Employee Name
    <input type="text" name="employeeName" value="<%= employee.name %>" readonly>
  </label><br>
  <label>
    <input type="hidden" name="employeeId" value="<%= employee._id %>" readonly>
  </label>
  <button type="submit">Update lab results</button> 
</form>

<%- include('../partials/footer') %>
